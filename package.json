{
  "name": "hono-unm-api-prod",
  "version": "1.0.6",
  "description": "基于 Hono 的网易云解灰 API 服务",
  "type": "module",
  "main": "dist/server.js",
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "tsc && node --experimental-specifier-resolution=node dist/server.js",
    "start": "node dist/server.js",
    "build": "tsc",
    "lint": "eslint --config eslint.config.mjs \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist",
    "prestart": "npm run build",
    "predev": "echo \"使用 ts-node-dev 启动开发服务器\""
  },
  "keywords": [
    "hono",
    "api",
    "unblockneteasemusic",
    "typescript"
  ],
  "author": "API Architect",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.12.0",
    "@hono/zod-validator": "^0.3.0",
    "@unblockneteasemusic/server": "^0.27.3",
    "dotenv": "^16.5.0",
    "hono": "^4.7.10",
    "pino": "^9.4.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@hono/eslint-config": "^2.0.1",
    "@types/node": "^20.15.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vercel/node": "^5.1.16",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "pino-pretty": "^11.3.0",
    "prettier": "^3.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.x",
    "bun": ">=1.0.0"
  }
}