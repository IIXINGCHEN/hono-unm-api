# 服务配置
NODE_ENV=development
PORT=5678
HOST=127.0.0.1

# 日志级别 (development: debug, info)
LOG_LEVEL=debug

# CORS 配置 (多个用逗号分隔)
# 测试环境允许所有源
ALLOWED_ORIGINS=*

# API 代理 (可选, 如果为空则不启用)
PROXY_URL=

# 外部音乐 API
EXTERNAL_MUSIC_API_URL=https://music-api.gdstudio.xyz/api.php

# UnblockNeteaseMusic 服务特定配置
ENABLE_FLAC=true

# 应用版本 (通常从 package.json 读取)
APP_VERSION=1.0.6

# 服务名称 (用于日志)
APP_NAME=hono-unm-api-test

# 安全配置
# 速率限制配置 (每个IP在指定时间窗口内的最大请求数)
# 测试环境设置较高的限制
RATE_LIMIT_MAX=1000
# 速率限制时间窗口 (毫秒) - 5分钟
RATE_LIMIT_WINDOW_MS=300000

# 用于加密敏感数据的密钥 (32字节)
ENCRYPTION_KEY=test-encryption-key-for-development-only

# JWT密钥 (用于API认证，如果需要)
JWT_SECRET=test-jwt-secret-for-development-only

# 安全头配置
# 是否启用严格的内容安全策略
ENABLE_STRICT_CSP=true
